<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <!-- Bootstrap Core CSS -->
    @Styles.Render("~/Template/bower_components/bootstrap/dist/css/bootstrap.min.css")
    <!-- MetisMenu CSS -->
    @Styles.Render("~/Template/bower_components/metisMenu/dist/metisMenu.min.css")
    <!-- DataTables CSS -->
    @Styles.Render("~/Template/bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.css")
    <!-- DataTables Responsive CSS -->
    @Styles.Render("~/Template/bower_components/datatables-responsive/css/dataTables.responsive.css")
    <!-- Timeline CSS -->
    @Styles.Render("~/Template/dist/css/timeline.css")
    <!-- Custom CSS -->
    @Styles.Render("~/Template/dist/css/sb-admin-2.css")
    <!-- Morris Charts CSS -->
    @Styles.Render("~/Template/bower_components/morrisjs/morris.css")
    <!-- Custom Fonts -->
    @Styles.Render("~/Template/bower_components/font-awesome/css/font-awesome.min.css")

    @Styles.Render("~/Content/ticker-style.css")

    @Styles.Render("~/jqueryuiCss")

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div id="wrapper">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Href("~")">@Html.Raw(HRMBLL.AppSettings.AppName)</a>
            </div>
            
            <!-- /.navbar-header -->
            @Html.Partial("_TopMenuPartial")
            <!-- /.navbar-top-links -->
            @Html.Partial("_LeftMenuPartial")
            <!-- /.navbar-static-side -->
        </nav>
        <div id="page-wrapper">
            @RenderBody()
        </div>
        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/jqueryuiScript")
    @Scripts.Render("~/bundles/bootstrap")
    <!-- Metis Menu Plugin JavaScript -->
    @Scripts.Render("~/Template/bower_components/metisMenu/dist/metisMenu.min.js")

    <!-- DataTables JavaScript -->
    @Scripts.Render("~/Template/bower_components/DataTables/media/js/jquery.dataTables.min.js")
    @Scripts.Render("~/Template/bower_components/datatables-plugins/integration/bootstrap/3/dataTables.bootstrap.min.js")
    @Scripts.Render("~/Scripts/jquery.ticker.js")
    <!-- Custom Theme JavaScript -->
    @Scripts.Render("~/Template/dist/js/sb-admin-2.js")

    @RenderSection("scripts", required: false)


   
</body>
</html>
